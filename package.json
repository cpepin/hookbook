{
  "name": "@cpepin/hookbook",
  "repository": {
    "type": "git",
    "url": "https://github.com/cpepin/hookbook.git"
  },
  "version": "1.0.0",
  "description": "A collection of helpful React hooks!",
  "files": [
    "dist/*"
  ],
  "main": "bundle.umd.js",
  "module": "index.js",
  "scripts": {
    "build": "babel lib --out-dir dist && rollup --config rollup.config.js",
